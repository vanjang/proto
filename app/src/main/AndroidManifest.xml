<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cochipcho.conchip">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:name="com.cochipcho.conchip.Activities.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Proto">

        <activity
            android:name="com.cochipcho.conchip.Activities.ProfileNavHostActivity"
            android:theme="@style/AppTheme.ActionBar.Transparent">
            <intent-filter>
                <data
                    android:host="conchip-b1ba6.firebaseapp.com"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cochipcho.conchip.Activities.SplashActivity"
            android:theme="@style/AppTheme.ActionBar.Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cochipcho.conchip.Activities.SigninNavHostActivity"
            android:theme="@style/AppTheme.ActionBar.Transparent" />
        <activity android:name="com.cochipcho.conchip.Activities.MainTabBarActivity" /> <!-- [START firebase_service] -->

<!--        <service-->
